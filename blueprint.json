{
	"title": "apache",
	"description": "apache web Server",
	"configuration": {
		"DEBIAN_FRONTEND": "noninteractive",
		"ServerName": "^^fqdn^^",
		"port": 80,
		"ServerAdmin": "webmaster@^^domain_name^^"
	},
	"os_packages": {
		"adds": [
			"apache2"
		]
	},
	"bindings": [
		{
			"type": "embed",
			"descriptor": {
				"repository": "https://github.com/v2Blueprints/enginesd_debian_base"
			},
			"variables": {
				"locale": "^^locale^^",
				"charset": "^^charset^^"
			}
		}
	],
	"containers": [
		{
			"type": "lxd"
		}
	]
}
