{
	"title": "apache",
	"description": "apache web Server",
	"binding_anchor": {
		"ServerAdmin": "webmaster@^^domain.name^^",
		"ServerName": "^^domain.fqdn^^",
		"port": 80
	},
	"bindings": [
		{
			"type": "embed",
			"descriptor": {
				"repository": "https://github.com/MarkRatjens/enginesd_debian_base"
			}
		}
	],
	"os_packages": {
		"adds": [
			"apache2"
		]
	},
	"volumes": [
		{
			"destination": "/var/log/apache/",
			"source": "logs"
		}
	]
}
